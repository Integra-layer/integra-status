{
  "functions": {
    "api/health.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=10, stale-while-revalidate=20" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
